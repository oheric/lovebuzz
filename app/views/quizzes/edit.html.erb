<% provide(:title, "Edit user") %>
<h1>Update your profile</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @quiz do |f| %>
  <div>
  <%= f.label :name %><br>
  <%= f.text_field :name %>
</div>
<%= f.fields_for :questions do |builder| %>

  <%= builder.label :content, "Question" %><br>
  <%= builder.text_area :content, :rows => 3 %>

<% end %>

<div class="actions">
    <%= f.submit "Submit" %>
  </div>
  <% end %>
