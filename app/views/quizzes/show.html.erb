<h1>Quiz</h1>
<div>
  <strong> Name: </strong>
<%= @quiz.name %>
</div><br>

<ol>
<% @questions.each do |q| %>
  <li>
    <%= q.content %>
    <ul> 
    <% q.answers.each do |a| %>
      <li><%= a.content %></li>
    <% end %>
    </ul>
  </li>
  <% end %>
</ol>

<div>
    <%= link_to "Edit", edit_quiz_path(@quiz) %> |  
    <%= link_to "Destroy", @quiz, :confirm => 'Are you sure?', :method => :delete %> |  
    <%= link_to "Quizzes Page", quizzes_path %>  
</div>